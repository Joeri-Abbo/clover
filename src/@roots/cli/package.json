{
  "name": "@roots/clover",
  "description": "Clover CLI",
  "version": "2.0.0-next.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/roots/clover.git"
  },
  "homepage": "https://roots.io/clover",
  "bugs": {
    "url": "https://github.com/roots/clover/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "contributors": [
    {
      "name": "Kelly Mears",
      "url": "https://github.com/kellymears"
    }
  ],
  "bin": "./bin/clover.js",
  "files": [
    "lib/",
    "docs/",
    "bin/",
    "types/"
  ],
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "require": "./lib/index.js"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "clover",
    "plugins": [
      "@oclif/plugin-plugins",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-which",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "yarn g:build",
    "build:cjs": "yarn g:build:cjs",
    "build:esm": "yarn g:build:esm",
    "profile": "yarn g:profile",
    "profile:cjs": "yarn g:profile:cjs",
    "profile:esm": "yarn g:profile:esm",
    "lint": "yarn g:lint",
    "lint:eslint": "yarn g:lint:eslint",
    "lint:prettier": "yarn g:lint:prettier",
    "clean": "yarn g:clean",
    "clean:lib": "yarn g:clean:lib",
    "clean:modules": "yarn g:clean:modules",
    "clean:types": "yarn g:clean:types",
    "pkg": "yarn g:pkg",
    "pkg:check": "yarn g:pkg:check",
    "pkg:format": "yarn g:pkg:format"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.2",
    "eslint": "^7.28.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@endemolshinegroup/cosmiconfig-typescript-loader": "^3.0.2",
    "@oclif/command": "^1.8.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-plugins": "^1.10.0",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@oclif/plugin-which": "^1.0.3",
    "@roots/clover-core": "workspace:*",
    "@roots/clover-dashboard": "workspace:*",
    "arr-rotate": "^1.0.0",
    "autobind-decorator": "^2.4.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "cosmiconfig": "^7.0.0",
    "enquirer": "^2.3.6",
    "esm": "^3.2.25",
    "execa": "^5.1.1",
    "find-plugins": "^1.1.7",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.3",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "^0.10.0",
    "human-format": "^0.11.0",
    "ink": "^3.0.8",
    "ink-big-text": "^1.2.0",
    "ink-gradient": "^2.0.0",
    "ink-link": "^2.0.0",
    "ink-select-input": "^4.2.0",
    "ink-spinner": "^4.0.2",
